.comprehensive .single .inner {
  padding-top: 20rpx;
}

.comprehensive .single .inner .item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-bottom: 20rpx;
}

.comprehensive .single .inner .item .left {
  width: 70%;
}

.comprehensive .single .inner .item .left .songName {
  display: flex;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.comprehensive .single .inner .item .left .songName .name {
  font-size: 34rpx;
  margin-right: 10rpx;
}

.comprehensive .single .inner .item .left .songName .original {
  font-size: 34rpx;
  color: #666;
}

.comprehensive .single .inner .item .left .center {
  display: flex;
  padding: 16rpx 0;
}

.comprehensive .single .inner .item .left .center .tag {
  font-size: 18rpx;
  color: #c20c0c;
  border: 2rpx solid #c20c0c;
  margin-right: 10rpx;
  padding: 0 4rpx;
}

.comprehensive .single .inner .item .left .center .singerName {
  font-size: 24rpx;
  color: #666;
}

.comprehensive .single .inner .item .left .alia {
  font-size: 24rpx;
  color: #666;
}

.comprehensive .single .inner .item .right {
  display: flex;
}

.comprehensive .single .inner .item .right .playImg {
  width: 50rpx;
  height: 50rpx;
  margin-right: 30rpx;
}

.comprehensive .single .inner .item .right .playImg image {
  width: 100%;
  height: 100%;
}

.comprehensive .single .inner .item .right .menuImg {
  width: 50rpx;
  height: 50rpx;
}

.comprehensive .single .inner .item .right .menuImg image {
  width: 100%;
  height: 100%;
}

.comprehensive .single .inner .moreText {
  text-align: center;
  color: #666;
  margin-top: 20rpx;
}

