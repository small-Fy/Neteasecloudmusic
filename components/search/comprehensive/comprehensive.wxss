.comprehensive {
  padding: 20rpx;
}

.comprehensive .single {
  margin: 40rpx 0;
}

.comprehensive .single .top {
  display: flex;
  justify-content: space-between;
}

.comprehensive .single .top .left {
  font-size: 36rpx;
  font-weight: 700;
}

.comprehensive .single .top .right {
  display: flex;
  padding: 6rpx 14rpx;
  border: 2rpx solid #999;
  border-radius: 30rpx;
}

.comprehensive .single .top .right .icon {
  margin-right: 8rpx;
}

