.searchInner {
  padding: 20rpx 30rpx;
  height: 1000rpx;
}

.searchInner .tabs{
  position: fixed;
  left: 0;
  right: 0;
  top:80rpx
}

.searchInner .top {
  display: flex;
  justify-content: space-between;
  margin-bottom: 40rpx;
  position: fixed;
  left: 0;
  right: 0;
  top: 20rpx
}

.searchInner .top .left {
  display: flex;
}

.searchInner .top .left .search {
  width: 500rpx;
  padding-bottom: 20rpx;
  border-bottom: 2rpx solid #666;
}

.searchInner .top .left .icon-left {
  width: 50rpx;
  height: 50rpx;
  margin-right: 40rpx;
}


.searchInner .top .icon-right {
  width: 50rpx;
  height: 50rpx;
}

.searchInner .top .icon-right image {
  width: 100%;
  height: 100%;
}